<?xml version="1.0" encoding="utf-8"?>
<mx:HBox xmlns:mx="http://www.adobe.com/2006/mxml"
	width="100%"
	height="100%">
	
	<mx:Script>
		<![CDATA[
			[Bindable]
			private var labelText:String;
			[Bindable]
			private var image:String;
		]]>
	</mx:Script>
	
	<mx:Label text="{labelText}"/>
	<mx:Image source="{image}"/>
</mx:HBox>
